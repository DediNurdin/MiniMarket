    Private Sub LogoutToolStripMenuItem_Click(ByVal sender As Object, ByVal e As System.EventArgs) Handles LogoutToolStripMenuItem.Click
        Dim Tanya = MessageBox.Show("Anda akan logout dari aplikasi !", "Peringatan !", MessageBoxButtons.YesNo, MessageBoxIcon.Warning)
        If Tanya = Windows.Forms.DialogResult.Yes Then
            Me.Hide()
            FrmLogin.Show()
            FrmLogin.KdPenggunaTxt.Text = ""
            FrmLogin.PswTxt.Text = ""
            FrmLogin.LevelTxt.Text = ""
        End If
    End Sub
